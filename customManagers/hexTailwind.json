{
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "npm",
      "depNameTemplate": "tailwindcss",
      "fileMatch": [
        "(^|/)config\\.exs$"
      ],
      "matchStrings": [
        "config :tailwind,\\s+version: \"(?<currentValue>[^\"]+)\""
      ]
    }
  ]
}
